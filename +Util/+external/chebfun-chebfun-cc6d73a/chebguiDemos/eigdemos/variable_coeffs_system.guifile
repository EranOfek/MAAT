# N.lbc = @(u,v) [u;v];
# N.rbc = @(u,v) [u;v];
# [V, D] = eigs(N, 6);
'System with variable coefficients'
'system'
domain = '[-2 2]';
DE = {'u" + u*x + v  = lambda*u';'v" + sin(x)*u = lambda*v'};
BC = {'u(-2) = 0';'v(-2) = 0' ; 'u(2)  = 0';'v(2)  = 0'};
type = 'eig';
